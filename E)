using System;
using System.Collections.Generic;

class Program
{
    static void Main()
    {
        List<int> numeros = new List<int> { 10, 20, 30, 40, 50 };
        int soma = 0;

        foreach (int numero in numeros)
        {
            soma += numero;
        }

        double media = (double)soma / numeros.Count;
        Console.WriteLine("A média dos números é: " + media);
    }
}
